{
  "name": "@codebase/universal",
  "version": "0.0.8",
  "description": "A universal library built by TOKTOKHAN.DEV",
  "author": "TOKTOKHAN.DEV <fe-system@toktokhan.dev>",
  "license": "ISC",
  "type": "module",
  "homepage": "https://www.codebase.page/docs/universal",
  "repository": {
    "type": "git",
    "url": "https://github.com/TOKTOKHAN-DEV/codebase/tree/main/packages/universal"
  },
  "bugs": {
    "url": "https://github.com/TOKTOKHAN-DEV/codebase/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "!dist/*.map"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "pnpm build && node dist/index.js",
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "play": "pnpm build:watch & node --watch play/playground.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "api-extractor": "api-extractor run --local --verbose"
  },
  "keywords": [
    "universal",
    "library",
    "toktokhan_dev"
  ],
  "devDependencies": {
    "@codebase/ts-config": "workspace:*",
    "@types/lodash": "^4.17.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
