{
  "name": "@codebase/github",
  "version": "0.0.12",
  "description": "A module to easily interact with the GitHub API using Octokit.",
  "author": "TOKTOKHAN.DEV <fe-system@toktokhan.dev>",
  "license": "ISC",
  "type": "module",
  "homepage": "https://www.codebase.page/docs/github",
  "repository": {
    "type": "git",
    "url": "https://github.com/TOKTOKHAN-DEV/codebase/blob/main/packages/github",
    "directory": "packages/github"
  },
  "bugs": {
    "url": "https://github.com/TOKTOKHAN-DEV/codebase/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "!dist/*.map"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "pnpm build && node dist/index.js",
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "api-extractor": "api-extractor run --local --verbose"
  },
  "keywords": [
    "github",
    "api",
    "octokit",
    "module",
    "integration",
    "repository",
    "automation"
  ],
  "devDependencies": {
    "@codebase/ts-config": "workspace:*",
    "@types/lodash": "^4.17.4"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@codebase/node": "workspace:*",
    "@codebase/universal": "workspace:*",
    "lodash": "^4.17.21",
    "matcher": "^5.0.0"
  }
}
