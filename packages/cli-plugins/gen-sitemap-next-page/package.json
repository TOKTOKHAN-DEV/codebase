{
  "name": "@codebase/cli-plugin-gen-sitemap-next-page",
  "version": "0.0.11",
  "description": "A CLI plugin for generating sitemaps for Next.js pages built by TOKTOKHAN.DEV",
  "author": "TOKTOKHAN.DEV <fe-system@toktokhan.dev>",
  "license": "ISC",
  "type": "module",
  "homepage": "https://www.codebase.page/docs/docs/tokript/Offical%20Plugins/gen-sitemap-next-page",
  "repository": {
    "type": "git",
    "url": "https://github.com/TOKTOKHAN-DEV/codebase/tree/main/packages/cli-plugins/gen-sitemap-next-page"
  },
  "bugs": {
    "url": "https://github.com/TOKTOKHAN-DEV/codebase/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "!dist/*.map"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "play": "pnpm build:watch & node --watch play/playground.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "api-extractor": "api-extractor run --local --verbose"
  },
  "keywords": [
    "cli",
    "plugin",
    "next.js",
    "sitemap",
    "generation",
    "toktokhan_dev"
  ],
  "dependencies": {
    "@codebase/cli": "workspace:*",
    "@codebase/node": "workspace:*",
    "@codebase/universal": "workspace:*",
    "globby": "^13.2.2",
    "lodash": "^4.17.21",
    "matcher": "^5.0.0"
  },
  "devDependencies": {
    "@codebase/ts-config": "workspace:*",
    "@types/lodash": "^4.17.0"
  }
}
